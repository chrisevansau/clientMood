<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="/web/css/bootstrap.min.css" rel="stylesheet" media="screen">
        <style>
      body {
        padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
      }
    </style>
    <link href="/web/css/bootstrap-responsive.css" rel="stylesheet">

        {% block stylesheets %}{% endblock %}
        {% javascripts '../web/js/*' %}

      <script src="{{ asset_url }}" type="text/javascript"></script>

{% endjavascripts %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="/">Balance Client Mood Board</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li class="active"><a href="{{ path('client') }}">Client</a></li>
              <li><a href="{{ path('projectmanager') }}">Project Manager</a></li>
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">

        
     
    
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </div>
        <script src="http://code.jquery.com/jquery-latest.js"></script>
<script src="/web/js/bootstrap.min.js"></script>
    </body>
</html>
